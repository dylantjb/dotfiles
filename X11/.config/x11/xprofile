#!/bin/sh

setxkbmap gb
sxhkd &
xset r rate 300 50
xmousepasteblock
xrdb -merge ~/.config/x11/xresources
davmail ~/.local/share/davmail/davmail.properties &
background
picom -b
unclutter &
redshift -l 51.5:0 &
xset s 1200
bluetoothctl on
xautolock -detectsleep -time 20 -corners 00+- -locker "slock" -killtime 20 -killer "sudo systemctl suspend" & 
